{
  "expo": {
    "name": "CTP",
    "slug": "ctp",
    "version": "1.3",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dheshadev.ctp",
      "infoPlist": {
        "UIBackgroundModes": ["remote-notification"],
        "NSHealthShareUsageDescription": "Allow CTP to read your health and fitness data to track your activities and progress in challenges.",
        "NSHealthUpdateUsageDescription": "Allow CTP to write your health and fitness data to track your activities and progress in challenges."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#000000"
      },
      "package": "com.dheshadev.ctp",
      "permissions": [
        "android.permission.RECEIVE_BOOT_COMPLETED",
        "android.permission.VIBRATE",
        "android.permission.SCHEDULE_EXACT_ALARM",
        "android.permission.health.READ_STEPS",
        "android.permission.health.READ_DISTANCE",
        "android.permission.health.READ_HEART_RATE",
        "android.permission.health.READ_EXERCISE",
        "android.permission.health.READ_SLEEP",
        "android.permission.health.READ_FLOORS_CLIMBED",
        "android.permission.health.READ_ACTIVE_CALORIES_BURNED"
      ]
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/images/icon.png"
    },
    "extra": {
      "eas": {
        "projectId": "ace5179f-9d07-4a93-86cb-af3735bf01ef"
      }
    },
    "plugins": [
      "expo-router",
      [
        "expo-build-properties",
        {
          "android": {
            "minSdkVersion": 26,
            "compileSdkVersion": 33,
            "targetSdkVersion": 33,
            "buildToolsVersion": "33.0.0"
          }
        }
      ],
      "expo-notifications",
      "./androidManifestPlugin.js"
    ]
  }
}
